{
  "name": "brass-runtime",
  "version": "1.2.1",
  "description": "Effect runtime utilities for TypeScript",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "keywords": [
    "runtime",
    "functional",
    "effect-system",
    "algebraic-effects",
    "async",
    "effects",
    "fiber",
    "fibers",
    "concurrency",
    "abortcontroller",
    "cancellation",
    "structured-concurrency",
    "typescript",
    "runtime"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "dev": "ts-node-dev src/examples/demo.ts",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node -e \"import fs from 'fs'; if (fs.existsSync('dist/index.cjs')) fs.unlinkSync('dist/index.cjs'); fs.renameSync('dist/index.js','dist/index.cjs')\"",
    "test": "tsx src/examples/index.ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "conventional-changelog-conventionalcommits": "^7.x.x",
    "semantic-release": "^25.0.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
